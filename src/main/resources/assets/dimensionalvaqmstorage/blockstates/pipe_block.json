{
  "multipart": [
    {
      "apply": {
        "model": "dimensionalvaqmstorage:block/pipeblock"
      }
    },
    {
      "when": {
        "pipe_connection_north": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:block/pipe_connection_north"
      }
    },
    {
      "when": {
        "pipe_connection_east": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:block/pipe_connection_east",
        "y": 90
      }
    },
    {
      "when": {
        "pipe_connection_south": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:block/pipe_connection_south",
        "x": 180
      }
    },
    {
      "when": {
        "pipe_connection_west": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:block/pipe_connection_west",
        "y": 270
      }
    },
    {
      "when": {
        "pipe_connection_up": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:block/pipe_connection_up",
        "x": 270
      }
    },
    {
      "when": {
        "pipe_connection_down": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:block/pipe_connection_down",
        "x": 90
      }
    }
  ]
}