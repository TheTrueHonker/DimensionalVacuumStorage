{
  "multipart": [
    {
      "apply": {
        "model": "dimensionalvaqmstorage:block/pipeblock"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:blocks/machines/pipe_connection_north"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:blocks/machines/pipe_connection_east",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:blocks/machines/pipe_connection_south",
        "x": 180
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:blocks/machines/pipe_connection_west",
        "y": 270
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:blocks/machines/pipe_connection_up",
        "x": 270
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "dimensionalvaqmstorage:blocks/machines/pipe_connection_down",
        "x": 90
      }
    }
  ]
}